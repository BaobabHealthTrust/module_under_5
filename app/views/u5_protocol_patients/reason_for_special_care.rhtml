<select id='1.8' name='concept[Any other reason for special care ]' helpText='Any other reason for special care ' pos='7'  tt_onLoad='__$("nextButton").style.display = "block"; showCategory("Reason For Special Care"); __$("category").style.fontSize = "30px"' >
<option></option>
<option value='Yes'>Yes</option>
 <option value='No'>No</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				Specify other reason for special care 
			</td>
			<td>
<input type='text' id='1.8.2.1'  name='concept[Specify]' helpText='Specify other reason for special care '  condition='__$("1.8").value == "Yes"'  pos='8'  tt_onLoad='__$("nextButton").style.display = "block"; showCategory("Reason For Special Care"); __$("category").style.fontSize = "30px"' />
			</td>
		</tr>
	</table>
</form>
